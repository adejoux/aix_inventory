.row
  .span12
    %h1= @report.name
    %table{ :id => "report_datatable", :class => "table table-striped", :data => { :source => "#{ report_url(format: "json") }" } }
      %thead
        %tr
          %th server
          - @columns.each do |column|
            %th=column
      %tbody

    = link_to "back", reports_path, class: "btn btn-danger"
